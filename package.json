{
  "version": "2.1.1",
  "name": "alf-validator",
  "description": "Extremely fast API Log Format (ALF) validator using JSON Schema",
  "author": "Ahmad Nassri <ahmad@mashape.com> (https://www.mashape.com/)",
  "homepage": "https://github.com/Mashape/alf-validator",
  "repository": "Mashape/alf-validator",
  "license": "MIT",
  "main": "src/index.js",
  "bin": "bin/alf-validator",
  "keywords": [
    "alf",
    "har",
    "http",
    "archive",
    "validate",
    "validator"
  ],
  "engines": {
    "node": ">=0.10"
  },
  "files": [
    "bin",
    "src"
  ],
  "bugs": {
    "url": "https://github.com/Mashape/alf-validator/issues"
  },
  "scripts": {
    "pretest": "standard && echint",
    "test": "mocha",
    "posttest": "npm run coverage",
    "coverage": "istanbul cover --dir coverage _mocha -- -R dot",
    "codeclimate": "codeclimate < coverage/lcov.info"
  },
  "echint": {
    "ignore": [
      "coverage/**"
    ]
  },
  "devDependencies": {
    "codeclimate-test-reporter": "0.0.4",
    "echint": "^1.1.1",
    "istanbul": "^0.3.14",
    "mocha": "^2.2.5",
    "require-directory": "^2.1.0",
    "should": "^6.0.3",
    "standard": "^3.11.1"
  },
  "dependencies": {
    "bluebird": "^2.9.26",
    "chalk": "^1.0.0",
    "commander": "^2.8.1",
    "har-validator": "^1.7.1",
    "is-my-json-valid": "^2.12.0",
    "stringify-clone": "^1.1.0"
  }
}
